{
    "name": "studocu/codeception-mobileemulation",
    "description": "WebDriver mobile emulation switcher for Codeception",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "codeception/codeception": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Module\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codeception\\Module\\MobileEmulation\\Test\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "vendor/bin/codecept run --steps",
        "cs": "phpcs --standard=PSR2 -p src/",
        "cs-fix": "phpcbf --standard=PSR2 -p src/"
    }
}
